@model IEnumerable<ClearCity.Models.Plan>

@{
    ViewBag.Title = "PlanDay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>План на день</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.House.HouseId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Team.TeamName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.House.HouseId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Team.TeamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
        </tr>
    }

</table>

@Html.ActionLink("Сформировать отчет", "ReportDay")